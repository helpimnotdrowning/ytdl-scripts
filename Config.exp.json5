{
	// DO NOT USE WINDOWS BACKSLASHES ( \ ) IN THIS FILE
	// ALWAYS USE NORMAL SLASHES ( / )

	// This is the sample file! Copy this to Config.json5 and refer to the
	// README before modify these settings.
	
	"OutputBase": "/media/ytdl",
	"YTDLPOutputFormat": "%(uploader)s/%(upload_date)s - %(id)s - %(title)s/%(title)s [%(id)s] f%(format_id)s.%(ext)s",
	
	"NoCookies": false,
	"AlwaysUseCookiesFile": false,
	"CookiesFilePath": "/media/ytdl/cookies.txt",
	"CookiesBrowser": "firefox",
	"JsRuntime": "deno",

	"ArchiveSleepMin": 180,
	"ArchiveSleepMax": 600,
	"ArchiveDataSleep": 10,
	
	// Double-check README before changing these numbers!
	"MediaConcurrentFragments": 5,
	"DataConcurrentFragments": 4
}
